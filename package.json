{
  "name": "adaptivai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint --max-warnings 999999",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build",
    "smoke": "npm run typecheck && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "hardening:checks": "npx tsx scripts/hardening-checks.ts",
    "setup": "npx prisma generate && npx prisma migrate dev --name init && npx tsx prisma/seed.ts && next dev",
    "db:generate": "npx prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    "pwa:icons": "node scripts/generate-pwa-icons.mjs",
    "generate:icons": "node scripts/generate-icons.mjs",
    "cap:add": "npx cap add",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev"
  },
