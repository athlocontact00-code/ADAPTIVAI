    "next-auth": "^5.0.0-beta.25",
    "next-intl": "^4.8.2",
    "react": "^19.0.0",
    "react-day-picker": "^9.4.4",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
